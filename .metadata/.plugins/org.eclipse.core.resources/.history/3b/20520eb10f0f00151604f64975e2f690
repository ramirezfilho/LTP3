	public void setValorHora(double valorHora) {
		this.valorHora = valorHora;
	}



	@Override
	public double getSalario(int mes, int ano) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int getTipo() {
		return HORISTA;
	}
	
	
	
	
	
}

package dados;

import java.util.ArrayList;

public class Horista extends Funcionario {

	private double valorHora;
	private ArrayList<HorasTrabalhadas> listahoras= new ArrayList<>();
	public Horista(String nome, String cpf, int matricula, double valorHora) {
		super(nome, cpf, matricula);
		this.valorHora = valorHora;
	}
	
	
	
}
